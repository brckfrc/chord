  <ItemGroup>
    <Compile Remove="ChordAPI.Tests\**" />
    <EmbeddedResource Remove="ChordAPI.Tests\**" />
    <None Remove="ChordAPI.Tests\**" />
  </ItemGroup>

</Project>
